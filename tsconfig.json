{
  "include": [
    "src/index.ts"
  ],
  "exclude": [
    "node_modules"
  ],
  "compilerOptions": {
    "target": "ES2019",
    "module": "ES6",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "declaration": true,
    "noImplicitAny": false,
    "removeComments": true,
    "noLib": false,
    "listFiles": false,
    "strict": true,
    "strictNullChecks": true,
    "incremental": true
  }
}

